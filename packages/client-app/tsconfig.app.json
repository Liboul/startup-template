{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc",
    "types": ["node"]
  },
  "files": ["../../node_modules/@nrwl/react/typings/image.d.ts"],
  "exclude": [
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "jest.config.ts",
    "vite.config.ts",
    "vite.config.mts",
    "vitest.config.ts",
    "vitest.config.mts",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/*.test.tsx",
    "**/*.spec.tsx",
    "**/*.test.js",
    "**/*.spec.js",
    "**/*.test.jsx",
    "**/*.spec.jsx"
  ],
  "include": ["**/*.js", "**/*.jsx", "**/*.ts", "**/*.tsx"],
  "references": [
    {
      "path": "../api/tsconfig.lib.json"
    },
    {
      "path": "../auth/tsconfig.lib.json"
    },
    {
      "path": "../data-access/tsconfig.lib.json"
    },
    {
      "path": "../ui/tsconfig.lib.json"
    }
  ]
}
