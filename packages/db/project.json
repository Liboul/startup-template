{
  "name": "@startup-template/db",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/db",
  "projectType": "library",
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./packages/db/start.sh"
      }
    },
    "prisma": {
      "command": "prisma",
      "options": {
        "cwd": "packages/db"
      }
    },
    "generate-types": {
      "command": "prisma generate",
      "options": {
        "cwd": "packages/db"
      }
    },
    "migrate": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "packages/db"
      }
    },
    "studio": {
      "command": "prisma studio",
      "options": {
        "cwd": "packages/db"
      }
    },
    "deploy": {
      "command": "prisma migrate deploy",
      "options": {
        "cwd": "packages/db"
      }
    }
  },
  "tags": []
}
